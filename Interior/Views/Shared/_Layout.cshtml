<!--css3Animations promo-product-->
<script>
    window.sr = ScrollReveal();
    sr.reveal('.right-animation', {

        duration: 1000,
        beforeReveal: function () {
            $(".right-animation").addClass("animated fadeInRight");
        },
        afterReveal: function(){
            $(".right-animation").removeClass('animated fadeInRight');
        }
    });

    sr.reveal('.left-animation', {

        duration: 1000,
        beforeReveal: function () {
            $(".left-animation").addClass("animated fadeInLeft");
        },
        afterReveal: function(){
            $(".left-animation").removeClass('animated fadeInLeft');
        }
    });
</script>
